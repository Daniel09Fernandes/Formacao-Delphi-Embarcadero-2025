unit ClasseProduto;

interface

uses
  System.Generics.Collections;

type
  TProduto = class
  private
    FID: Integer;
    FNome: string;
    FDescricao: string;
    FPreco: Real;
    FEstoque: Integer;

  end;

{
ID,
    NOME,
    DESCRICAO,
    PRECO,
    ESTOQUE,
    CATEGORIA
}

implementation

end.
