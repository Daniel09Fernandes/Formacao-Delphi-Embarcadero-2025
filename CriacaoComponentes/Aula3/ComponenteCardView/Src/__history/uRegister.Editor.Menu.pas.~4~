unit uRegister.Editor.Menu;

interface

uses
  DesignEditors, DesignIntf, VCL.Dialogs, SysUtils;

type
  TEditorMenu = class(TComponentEditor)
  public
    procedure ExecuteVerb(index: Integer) override;
    procedure GetVerb(index: Integer) override;
    procedure GetVerbCount(index: Integer) override;
  end;


implementation

{ TEditorMenu }

procedure TEditorMenu.ExecuteVerb(index: Integer);
begin
  inherited;

end;

procedure TEditorMenu.GetVerb(index: Integer);
begin
  inherited;

end;

function TEditorMenu.GetVerbCount: integer;
begin
  inherited;
  Result := 1;
end;

end.
